                                results.classList.add('fade-in');\
                                updateShippingCost();\
                            \} else \{\
                                noResults.style.display = 'block';\
                                noResults.classList.add('fade-in');\
                                currentProduct = null;\
                            \}\
                        \}, 500);\
                    \}\
                \}, 100);\
            \});\
            \
            applyActualBtn.addEventListener('click', function() \{\
                const manualWeight = parseFloat(document.getElementById('manual-actual-weight').value);\
                if (manualWeight && manualWeight > 0) \{\
                    if (!currentProduct) \{\
                        currentProduct = \{\
                            name: 'Custom Product',\
                            actualWeight: manualWeight,\
                            dimensionalWeight: 0\
                        \};\
                        displayProductInfo(currentProduct);\
                        results.style.display = 'block';\
                    \} else \{\
                        currentProduct.actualWeight = manualWeight;\
                        document.getElementById('actual-weight').textContent = `$\{manualWeight\} lbs`;\
                    \}\
                    updateShippingCost();\
                \} else \{\
                    alert('Please enter a valid weight');\
                \}\
            \});\
            \
            applyDimensionalBtn.addEventListener('click', function() \{\
                const manualWeight = parseFloat(document.getElementById('manual-dimensional-weight').value);\
                if (manualWeight && manualWeight > 0) \{\
                    if (!currentProduct) \{\
                        currentProduct = \{\
                            name: 'Custom Product',\
                            actualWeight: 0,\
                            dimensionalWeight: manualWeight\
                        \};\
                        displayProductInfo(currentProduct);\
                        results.style.display = 'block';\
                    \} else \{\
                        currentProduct.dimensionalWeight = manualWeight;\
                        document.getElementById('dimensional-weight').textContent = `$\{manualWeight\} lbs`;\
                    \}\
                    updateShippingCost();\
                \} else \{\
                    alert('Please enter a valid weight');\
                \}\
            \});\
            \
            calculateDimensionalBtn.addEventListener('click', function() \{\
                const length = prompt('Enter package length (inches):');\
                const width = prompt('Enter package width (inches):');\
                const height = prompt('Enter package height (inches):');\
                \
                if (length && width && height) \{\
                    const dimensionalWeight = (length * width * height) / 139;\
                    const roundedWeight = Math.round(dimensionalWeight * 10) / 10;\
                    document.getElementById('manual-dimensional-weight').value = roundedWeight;\
                    alert(`Dimensional weight calculated (IATA): $\{roundedWeight\} lbs\\nFormula: ($\{length\} \'d7 $\{width\} \'d7 $\{height\}) / 139 = $\{roundedWeight\}`);\
                \}\
            \});\
            \
            calculateBtn.addEventListener('click', function() \{\
                if (!currentProduct) \{\
                    alert('Please search for a product or enter weights manually first');\
                    return;\
                \}\
                \
                const itemPrice = parseFloat(document.getElementById('item-price').value) || 0;\
                const taxPercentage = parseFloat(document.getElementById('tax-percentage').value) || 0;\
                \
                const selectedWeight = Math.max(currentProduct.actualWeight, currentProduct.dimensionalWeight);\
                let baseCost = selectedWeight < 1 ? 10 : selectedWeight * 9;\
                \
                const subtotal = itemPrice + baseCost;\
                const taxAmount = subtotal * (taxPercentage / 100);\
                const finalAmount = subtotal + taxAmount;\
                \
                document.getElementById('final-amount').textContent = finalAmount.toFixed(2);\
                finalPrice.style.display = 'block';\
                finalPrice.classList.add('fade-in');\
                \
                document.getElementById('breakdown-item-price').textContent = `$$\{itemPrice.toFixed(2)\}`;\
                document.getElementById('breakdown-shipping').textContent = `$$\{baseCost.toFixed(2)\}`;\
                document.getElementById('breakdown-tax').textContent = `$$\{taxAmount.toFixed(2)\}`;\
                document.getElementById('breakdown-total').textContent = `$$\{finalAmount.toFixed(2)\}`;\
                breakdown.style.display = 'block';\
                breakdown.classList.add('fade-in');\
            \});\
            \
            clearBtn.addEventListener('click', function() \{\
                productSearch.value = '';\
                document.getElementById('item-price').value = '';\
                document.getElementById('tax-percentage').value = '';\
                document.getElementById('manual-actual-weight').value = '';\
                document.getElementById('manual-dimensional-weight').value = '';\
                \
                results.style.display = 'none';\
                noResults.style.display = 'none';\
                finalPrice.style.display = 'none';\
                breakdown.style.display = 'none';\
                \
                currentProduct = null;\
            \});\
            \
            function displayProductInfo(product) \{\
                document.getElementById('product-name').textContent = product.name;\
                document.getElementById('actual-weight').textContent = `$\{product.actualWeight\} lbs`;\
                document.getElementById('dimensional-weight').textContent = `$\{product.dimensionalWeight\} lbs (IATA)`;\
                \
                const selectedWeight = Math.max(product.actualWeight, product.dimensionalWeight);\
                const weightType = selectedWeight === product.actualWeight ? 'Actual' : 'Dimensional';\
                document.getElementById('selected-weight').textContent = `$\{selectedWeight\} lbs ($\{weightType\})`;\
            \}\
            \
            function updateShippingCost() \{\
                if (!currentProduct) return;\
                \
                const selectedWeight = Math.max(currentProduct.actualWeight, currentProduct.dimensionalWeight);\
                let baseCost = selectedWeight < 1 ? 10 : selectedWeight * 9;\
                \
                document.getElementById('base-cost').textContent = `$$\{baseCost.toFixed(2)\}`;\
                \
                const shippingTierElement = document.getElementById('shipping-tier');\
                if (selectedWeight < 1) \{\
                    shippingTierElement.textContent = "Shipping Tier: $10 Standard Fee (less than 1 lb)";\
                    shippingTierElement.style.backgroundColor = 'rgba(168, 216, 234, 0.4)';\
                \} else \{\
                    shippingTierElement.textContent = `Shipping Tier: $9 per pound ($\{selectedWeight\} lbs \'d7 $9 = $$\{baseCost.toFixed(2)\})`;\
                    shippingTierElement.style.backgroundColor = 'rgba(255, 126, 95, 0.2)';\
                \}\
                \
                const weightType = selectedWeight === currentProduct.actualWeight ? 'Actual' : 'Dimensional';\
                document.getElementById('selected-weight').textContent = `$\{selectedWeight\} lbs ($\{weightType\})`;\
            \}\
            \
            productSearch.addEventListener('keypress', function(e) \{\
                if (e.key === 'Enter') \{\
                    searchBtn.click();\
                \}\
            \});\
        \});\
    </script>\
